source "https://rubygems.org"

# Use modern Jekyll (compatible with Ruby 4.0)
gem "jekyll", "~> 4.3"

# Plugins
group :jekyll_plugins do
  gem "jekyll-feed"
  gem "jekyll-seo-tag"
  gem "jekyll-sitemap"
end

# Required for Ruby 3.4+
gem "csv"
gem "bigdecimal"
gem "base64"
gem "logger"

# Server
gem "webrick"
